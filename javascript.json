    "Import React": {
      "prefix": "ire",
      "body": "import React from 'react';$0"
    },
    "Named Import": {
      "prefix": "nim",
      "body": "import { $1 } from '$2';$0"
    },
    "Default Import": {
      "prefix": "dim",
      "body": "import $1 from '$2';$0"
    },
    "CommonJS Require": {
      "prefix": "rem",
      "body": "const $1 = require('$1$2');$0"
    },
    "routeHandler": {
      "prefix": "rhl",
      "body": "'/$1', (req, res) => {\n\t$0\n}",
      "description": "node.js route handler"
    },
